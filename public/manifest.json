{
  "name": "FOI bulk download plugin",
  "description": "The extension enables user to bulk download the links on FOI page.",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "downloads"
  ],
  "content_scripts": [
    {
      "matches": ["https://ods.foi.org.tw/"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}